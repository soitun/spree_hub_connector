<div id="confirm_delete">
  <div class="modal-header delete">
    <h6>Confirm Delete?</h6>
  </div>
  <div class="modal-body">
    <p><%= @warning %></p>
  </div>
  <div class="modal-footer">
    <div class="form-actions">
      <a href="/admin/<%= @klass %>/delete/<%= @identifier %>?confirm=true" class="button icon-trash delete">Delete</a>
      <span class="or">or</span>
      <a href="/admin/integration/cancel_dialog" class="button icon-cancel cancel">Cancel</a>
    </div>
  </div>
</div>
